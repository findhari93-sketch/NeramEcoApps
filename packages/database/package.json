{
  "name": "@neram/database",
  "version": "1.0.0",
  "private": true,
  "description": "Supabase database client and types for Neram Classes",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types/index.ts",
    "./queries": "./src/queries/index.ts"
  },
  "scripts": {
    "generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "push": "supabase db push",
    "pull": "supabase db pull",
    "studio": "supabase studio",
    "migrate": "supabase migration up",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
